{
    "schemaVersion": 1,
    "id": "fastconfigapi",
    "version": "${version}",
    "name": "Fast Config API",
    "description": "A mod that makes creating Configs easy and fast",
    "authors": [
        "Infinituum"
    ],
    "contact": {
        "sources": "https://github.com/Infinituum17/FastConfigAPI"
    },
    "license": "Infinituum License",
    "icon": "assets/fastconfigapi/icon.png",
    "environment": "*",
    "entrypoints": {
        "main": [
            "infinituum.fastconfigapi.fabric.FastConfigAPIFabric"
        ],
        "client": [
            "infinituum.fastconfigapi.fabric.FastConfigAPIFabric"
        ],
        "server": [
            "infinituum.fastconfigapi.fabric.FastConfigAPIFabric"
        ]
    },
    "mixins": [
        "fastconfigapi-fabric.mixins.json"
    ],
    "depends": {
        "minecraft": ">=${minecraft_version}",
        "void_lib": ">=${void_lib_version}"
    }
